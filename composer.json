{
  "name": "fusio/adapter-v8",
  "description": "Write the endpoint logic in javascript using the V8 engine",
  "homepage": "http://fusio-project.org",
  "keywords": ["fusio-adapter"],
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "fusio/engine": "^2.0",
    "psx/v8": "^0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "phpunit/dbunit": "^2.0",
    "pinepain/php-v8-stubs": "^0.1.1",
    "fusio/adapter-amqp": "^2.0",
    "fusio/adapter-beanstalk": "^2.0",
    "fusio/adapter-elasticsearch": "^2.0",
    "fusio/adapter-http": "^2.0",
    "fusio/adapter-memcache": "^2.0",
    "fusio/adapter-mongodb": "^2.0",
    "fusio/adapter-redis": "^2.0",
    "fusio/adapter-soap": "^2.0",
    "fusio/adapter-sql": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Fusio\\Adapter\\V8\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fusio\\Adapter\\V8\\Tests\\": "tests/"
    }
  }
}
