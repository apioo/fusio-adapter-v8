{
  "name": "fusio/adapter-v8",
  "description": "Write the endpoint logic in javascript using the V8 engine",
  "homepage": "http://fusio-project.org",
  "keywords": ["fusio-adapter"],
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "fusio/engine": "^0.4@dev",
    "psx/v8": "^0.1@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1",
    "phpunit/dbunit": "^1.3",
    "pinepain/php-v8-stubs": "^0.1.1"
  },
  "autoload": {
    "psr-4": {
      "Fusio\\Adapter\\V8\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fusio\\Adapter\\V8\\Tests\\": "tests/"
    }
  }
}
